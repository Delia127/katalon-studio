<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="com.kms.katalon.composer.integration.analytics"
   name="Analytics Integration Composer"
   version="1.0.0.qualifier"
   provider-name="KMS">
   <extension
         point="com.kms.katalon.preferences.ProjectSettingPages">
      <page
            class="com.kms.katalon.composer.integration.analytics.preferences.AnalyticsPreferencesPage"
            id="com.kms.katalon.composer.integration.analytics.page"
            name="Katalon TestOps">
      </page>
   </extension>

   <extension
         id="com.kms.katalon.composer.integration.analytics.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.kms.katalon.integration.analytics.preferences.AnalyticsPreferenceDefaultValueInitializer">
      </initializer>
   </extension>
   
</plugin>
