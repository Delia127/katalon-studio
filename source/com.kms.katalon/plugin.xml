<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="Katalon Studio"
            application="com.kms.katalon.application">
         <property
               name="appName"
               value="Katalon Studio">
         </property>
         <property
		     name="applicationCSS"
		     value="platform:/plugin/com.kms.katalon/css/default.css">
		   </property>
         <property 
         		name="applicationXMI" 
         		value="com.kms.katalon/Application.e4xmi"> 
	     </property>
	     <property
           name="lifeCycleURI"
           value="bundleclass://com.kms.katalon/com.kms.katalon.core.application.LifeCycleManager">
         </property>
         <property
		    name="rendererFactoryUri"
		    value="bundleclass://com.kms.katalon/com.kms.katalon.composer.components.CustomWorkbenchRendererFactory">
		</property>
         <property
               name="windowImages"
               value="icons/branding_16.png,icons/branding_32.png,icons/branding_48.png,icons/branding_64.png,icons/branding_128.png,icons/branding_256.png">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="aboutImage"
               value="icons/branding_128.png">
         </property>
         <property
               name="aboutText"
               value="%productBlurb">
         </property>
         <property
               name="startupForegroundColor"
               value="404040">
         </property>
         <property
               name="startupMessageRect"
               value="5,270,445,20">
         </property>
         <property
               name="startupProgressRect"
               value="0,290,450,10">
         </property>
      </product>
   </extension>

	<extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run class="com.kms.katalon.core.application.Application">
         </run>
      </application>
   </extension>
 <extension
       id="model"
       point="org.eclipse.e4.workbench.model">
    <processor
          apply="always"
          beforefragment="true"
          class="com.kms.katalon.processors.MenuProcessor">
    </processor>
 </extension>
 <extension
       point="org.eclipse.core.runtime.preferences">
    <initializer
          class="com.kms.katalon.composer.preferences.GeneralPreferenceDefaultValueInitializer">
    </initializer>
 </extension>
 <extension
       point="com.kms.katalon.preferences.PreferencePages">
    <page
          class="com.kms.katalon.composer.preferences.GeneralPreferencePage"
          id="com.kms.katalon.composer.preferences.GeneralPreferencePage"
          name="Katalon">
    </page>
 </extension>
</plugin>
