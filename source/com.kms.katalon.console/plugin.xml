<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="Katalon Studio Engine"
            application="com.kms.katalon.console.application">
         <property
               name="appName"
               value="Katalon Studio Engine">
         </property>
      </product>
   </extension>

   <extension
       id="application"
       name="Katalon Application"
       point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            visible="true">
         <run class="com.kms.katalon.application.Application">
         </run>
      </application>
   </extension>
</plugin>
