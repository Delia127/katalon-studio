<?xml version="1.0"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm" package-name="com.kms.katalon.entity.checkpoint">
    <java-types>
        <java-type name="DatabaseCheckpointSourceInfo">
            <xml-root-element name="source-info" />
            <xml-type prop-order="usingGlobalDBSetting secureUserAccount user password query" />
            <java-attributes>
                <!-- for database -->
                <xml-element java-attribute="usingGlobalDBSetting" name="is-using-global-db-setting" />
                <xml-element java-attribute="secureUserAccount" name="is-secure-user-account" />
                <xml-element java-attribute="user" />
                <xml-element java-attribute="password" />
                <xml-element java-attribute="query" cdata="true" />
            </java-attributes>
        </java-type>
    </java-types>
</xml-bindings>
