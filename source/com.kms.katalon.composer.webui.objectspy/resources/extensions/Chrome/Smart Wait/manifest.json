{
  "description": "",
  "manifest_version": 2,
  "name": "Katalon Wait",
  "version": "1.0.0",
  "homepage_url":"https://www.katalon.com/",
  "icons":
  {
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "contextMenus",
    "downloads",
    "webNavigation",
    "notifications",
    "http://*/",
    "https://*/",
    "storage",
    "unlimitedStorage",
    "<all_urls>",
    "debugger"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "common/browser-polyfill.js",
        "content/wait-injector.js"
      ],
      "match_about_blank": true,
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "content/wait.js"
  ],

  "offline_enabled": true
}
