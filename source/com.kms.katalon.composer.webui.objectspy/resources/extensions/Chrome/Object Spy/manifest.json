{
	"manifest_version": 2,
	"name": "Object spy",
	"description": "Spy element Chrome extension for qAutomate",
	"version": "1.0",
	"permissions": ["http://*/", "https://*/"],
	"content_scripts": [
		{
		"all_frames": true,
		"matches": ["http://*/*", "https://*/*"],
		"match_about_blank": true,
		"js": ["jquery-1.11.2.min.js", "jquery.color.js", "server.js", "common.js", "dom_inspector.js", "dom_collector.js"]
		}
	],
	"background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}