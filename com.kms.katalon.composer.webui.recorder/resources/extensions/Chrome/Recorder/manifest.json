{
	"manifest_version": 2,
	"name": "recorder",
	"description": "HTML Recorder Chrome extension for qAutomate",
	"version": "1.0",
	"permissions": ["http://*/", "tabs"],
	"content_scripts": [
		{
		"all_frames": true,
		"matches": ["http://*/*", "https://*/*"],
		"match_about_blank": true,
		"js": ["jquery-1.11.2.min.js", "server.js", "common.js", "dom_recorder.js"]
		}
	],
	"background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}